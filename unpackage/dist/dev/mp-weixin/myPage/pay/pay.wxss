.home {
  display: flex;
  flex-direction: column;
  background-color: #f1f1f1;
}
.home .head {
  display: flex;
  flex-direction: column;
  background-color: #fff;
  margin: 40rpx 40rpx;
  padding: 50rpx 50rpx;
  border-radius: 30rpx;
}
.home .head .selfInfo {
  display: flex;
  align-items: center;
  margin-bottom: 25rpx;
}
.home .head .selfInfo .pic {
  display: flex;
  height: 100rpx;
  width: 100rpx;
  border-radius: 30rpx;
}
.home .head .selfInfo .pic image {
  border: solid 5rpx gold;
  flex: 1;
  height: 100%;
  width: 100%;
  border-radius: 20rpx;
}
.home .head .selfInfo .text {
  margin-left: 40rpx;
}
.home .head .times {
  margin-top: 25rpx;
}
